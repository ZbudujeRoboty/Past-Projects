10 M1 = 0
20 OVRD 75
30 MOV P1 'DO DOMU
40 P2 = P1
50 P2.Z = 112
60 DLY 1
70 HOPEN 1
80 DLY 1
90 OPEN "COM1:" AS #1
100 INPUT #1,CMX$
110 IF CMX$="START"THEN
120 PRINT #1, "GOTOWY"
130 INPUT #1,X
140 INPUT #1,Y
150 INPUT #1,A
160 P2.X = X
170 P2.Y = Y
180 P2.A = RAD(A)
190 MOV P2, -40
200 PRINT #1, "JESTEM NAD OBIEKTEM"
210 OVRD 20
220 MVS P2
230 DLY 0.5
240 HCLOSE 1
250 DLY 0.5
260 MVS P2, -40
270 OVRD 75
280 MOV P1
290 DLY 1
300 HOPEN 1
310 DLY 0.5
320 DLY 3
330 ENDIF